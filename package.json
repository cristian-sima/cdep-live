{
  "version": "2.0.0",
  "scripts": {
    "eslint:client": "eslint --fix --ext=jsx --ext=js --ignore-pattern=client/conf.json --ignore-pattern=client/styles/** client/**",
    "eslint:server": "eslint --fix --ext=js -c server/.eslintrc.json --ignore-pattern=server/static/**  server/** ",
    "eslint": "npm run eslint:client && npm run eslint:server",
    "flow": "flow check --color=always",
    "test": "jest",
    "check": "npm-check-updates && npm update && flow-typed update",
    "test:watch": "jest --watch",
    "dev:client:start": "node conf/dev.js & node server.js",
    "dev:server:start": "babel-watch ./server/live.js",
    "dist:pre-build": "node conf/dist.js",
    "dist:client:build": "webpack --progress -p",
    "dist:server:clean": "rm -rf dist && mkdir dist",
    "dist:server:build": "babel server -d dist",
    "dist:build": "npm run dist:pre-build && npm run dist:server:build && npm run dist:client:build",
    "dist:serve": "forever start dist/live.js",
    "dist:stop": "forever stop dist/live.js"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-decorators-legacy",
          "react-html-attrs",
          "transform-class-properties",
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "transform-react-remove-prop-types",
          "transform-flow-strip-types"
        ]
      },
      "developmentTime": {
        "plugins": [
          "transform-decorators-legacy",
          "react-hot-loader/babel",
          "flow-react-proptypes",
          "react-html-attrs",
          "transform-class-properties"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cristian-sima/live.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 8.9.3"
  },
  "jest": {
    "testRegex": "(client|server)/.*-test.js$",
    "modulePaths": [
      "client"
    ]
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "redux-mock-store": "^1.3.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^10.0.0",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-alpha.6",
    "client-sessions": "^0.8.0",
    "css-loader": "^0.28.7",
    "dezalgo": "^1.0.3",
    "eslint": "^4.13.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.61.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "flow-typed": "^2.2.3",
    "font-awesome": "^4.7.0",
    "forever": "^0.15.3",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "jest-cli": "^21.2.1",
    "jest-immutable-matchers": "^1.5.0",
    "json-loader": "^0.5.7",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.19.3",
    "mongodb": "^2.2.33",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.7.2",
    "npm-check-updates": "^2.13.0",
    "postcss-loader": "^2.0.9",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.2.0",
    "react-history": "^0.18.2",
